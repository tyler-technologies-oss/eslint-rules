{
  "name": "@tylertech-eslint/eslint-plugin-angular-template",
  "version": "1.0.0",
  "description": "Shared angular template ESLint rules for Tyler Technologies.",
  "author": "Tyler Technologies, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyler-technologies-oss/eslint-rules.git",
    "directory": "packages/eslint-plugin-angular-template"
  },
  "type": "module",
  "scripts": {
    "build": "tsdown"
  },
  "keywords": [
    "eslint",
    "angular",
    "angular-eslint",
    "lint"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./configs/*": "./dist/configs/*.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@typescript-eslint/utils": "catalog:",
    "angular-eslint": "^19.8.0",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "@angular/core": ">=19"
  }
}
