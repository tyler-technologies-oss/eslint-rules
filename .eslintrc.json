{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-restricted-imports": [
            "error",
            {
                "paths": [
                    ""
                ],
                "patterns": [
                    "./../*"
                ],
                "message": "asd"
            }
        ]
    }
}